generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("TEST_DATABASE_URL")
}

model Book {
  id          String   @id @default(cuid())
  title       String   @db.VarChar(128)
  author      String   @db.VarChar(64)
  description String?  @db.Text
  createdAt   DateTime @default(now())

  @@map("book")
}

